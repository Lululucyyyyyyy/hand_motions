    "from PIL import Image\n",
    "import glob\n",
    "\n",
    "\n",
    "globpath = \"/ones/*.JPG\"\n",
    "for i, image in enumerate(glob.glob(globpath)):\n",
    "    with open(image, 'rb') as file:\n",
    "        img = Image.open(file)\n",
    "        img.rotate(270)\n",
    "        img.save('five' + str(i+53)+'.JPG')\n",
    "print('done')"
   ]
